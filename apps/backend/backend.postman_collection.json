              "raw": "{\n  \"name\": \"Updated UTG Opening Range\",\n  \"userId\": \"{{userId}}\",\n  \"handsRange\": [\n    {\n      \"carryoverFrequency\": 100,\n      \"label\": \"AA\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 100}\n      ]\n    },\n    {\n      \"carryoverFrequency\": 100,\n      \"label\": \"KK\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 100}\n      ]\n    },\n    {\n      \"carryoverFrequency\": 100,\n      \"label\": \"QQ\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 100}\n      ]\n    },\n    {\n      \"carryoverFrequency\": 100,\n      \"label\": \"AK\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 100}\n      ]\n    },\n    {\n      \"carryoverFrequency\": 80,\n      \"label\": \"AQ\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 80},\n        {\"type\": \"CALL\", \"frequency\": 20}\n      ]\n    }\n  ]\n}"
            },
