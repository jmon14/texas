    {
      "name": "Ranges",
      "item": [
        {
          "name": "Get All Ranges",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/ranges",
              "host": ["{{base_url}}"],
              "path": ["ranges"]
            },
            "description": "Retrieve all ranges in the system"
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response is an array\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.be.an('array');",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Get Range by ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/ranges/{{rangeId}}",
              "host": ["{{base_url}}"],
              "path": ["ranges", "{{rangeId}}"]
            },
            "description": "Retrieve a specific range by its ID"
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response has required fields\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('name');",
                  "    pm.expect(jsonData).to.have.property('userId');",
                  "    pm.expect(jsonData).to.have.property('handsRange');",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Get Ranges by User ID",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/ranges/user/{{userId}}",
              "host": ["{{base_url}}"],
              "path": ["ranges", "user", "{{userId}}"]
            },
            "description": "Retrieve all ranges for a specific user"
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response is an array\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.be.an('array');",
                  "});",
                  "",
                  "pm.test(\"All ranges belong to specified user\", function () {",
                  "    const jsonData = pm.response.json();",
                  "    const userId = pm.variables.get('userId');",
                  "    jsonData.forEach(range => {",
                  "        pm.expect(range.userId).to.equal(userId);",
                  "    });",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
        {
          "name": "Create Range",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"UTG Opening Range\",\n  \"userId\": \"{{userId}}\",\n  \"handsRange\": [\n    {\n      \"carryoverFrequency\": 100,\n      \"label\": \"AA\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 100}\n      ]\n    },\n    {\n      \"carryoverFrequency\": 100,\n      \"label\": \"KK\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 100}\n      ]\n    },\n    {\n      \"carryoverFrequency\": 100,\n      \"label\": \"QQ\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 100}\n      ]\n    },\n    {\n      \"carryoverFrequency\": 50,\n      \"label\": \"AK\",\n      \"actions\": [\n        {\"type\": \"RAISE\", \"frequency\": 100}\n      ]\n    }\n  ]\n}"
            },
            "url": {
              "raw": "{{base_url}}/ranges",
              "host": ["{{base_url}}"],
              "path": ["ranges"]
            },
            "description": "Create a new range with hand ranges and actions"
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test(\"Status code is 201\", function () {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test(\"Response contains success message\", function () {",
                  "    const responseText = pm.response.text();",
                  "    pm.expect(responseText).to.include('Range created successfully');",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ]
        },
