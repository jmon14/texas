{
  "name": "frontend",
  "version": "2.2.2",
  "description": "Quickly review data and obtain insights from it.",
  "scripts": {
    "build": "webpack --env ENVIRONMENT=production",
    "lint": "eslint --fix \"./src/**/*.{ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "openapi:backend": "npm run openapi:spec-backend && npm run openapi:pretty-backend && npm run openapi:generate-backend",
    "openapi:spec-backend": "curl http://localhost:3000/api-json -o openapi-backend-spec.json",
    "openapi:pretty-backend": "prettier --write openapi-backend-spec.json",
    "openapi:generate-backend": "openapi-generator-cli generate -i openapi-backend-spec.json -o backend-api -g typescript-axios --additional-properties=supportsES6=true",
    "start": "webpack serve",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": "Jorge Montero",
  "license": "ISC",
  "dependencies": {
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.8.5",
    "@reduxjs/toolkit": "1.8.5",
    "axios": "0.27.2",
    "js-cookie": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-hook-form": "7.34.2",
    "react-redux": "8.0.2",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.25.9",
    "@babel/preset-env": "7.25.9",
    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "7.25.9",
    "@babel/runtime": "7.25.9",
    "@chromatic-com/storybook": "3.2.4",
    "@emotion/babel-plugin": "11.10.2",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.9.3",
    "@fontsource/material-icons": "5.0.18",
    "@fontsource/roboto": "5.0.14",
    "@openapitools/openapi-generator-cli": "2.5.1",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-onboarding": "8.6.12",
    "@storybook/addon-webpack5-compiler-swc": "3.0.0",
    "@storybook/blocks": "8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-webpack5": "8.6.12",
    "@storybook/test": "8.6.12",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.1.0",
    "@types/js-cookie": "3.0.2",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "dotenv": "16.0.2",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.12.0",
    "html-webpack-plugin": "5.5.0",
    "jest": "29.1.1",
    "jest-environment-jsdom": "29.1.2",
    "lint-staged": "13.0.3",
    "msw": "2.3.4",
    "msw-storybook-addon": "2.0.4",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "storybook": "8.6.12",
    "ts-node": "10.8.1",
    "typescript": "4.7.4",
    "undici": "6.19.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
